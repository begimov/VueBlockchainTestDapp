  "source": "pragma solidity ^0.4.8;\n\ncontract Voting {\n    uint public counter;\n\n    function Voting() public {\n        counter = 1;\n    }\n\n    function increment() public {\n        counter += 1;\n    }\n\n    function getCounter() public returns (uint) {\n        return counter;\n    }\n}",
